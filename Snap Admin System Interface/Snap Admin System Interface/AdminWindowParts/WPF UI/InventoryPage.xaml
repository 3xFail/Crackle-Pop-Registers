<Page x:Class="Snap_Admin_System_Interface.AdminWindowParts.WPF_UI.InventoryPages.InventoryPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Snap_Admin_System_Interface.AdminWindowParts.WPF_UI"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="InventoryPage">

    <Grid>
        <StackPanel Grid.Column="1" Grid.ColumnSpan="4" Grid.Row="1" >
            <Button x:Name="AddBtn" Content="Add" Height="50" Click="AddBtn_Click"/>
            <Button x:Name="RemoveBtn" Content="Remove" Height="50" Click="RemoveBtn_Click"/>
            <Button x:Name="CatalogBtn" Content="Catalog" Height="50" Click="CatalogBtn_Click"/>
            <Button x:Name="InvBackBtn" Content="Back" Height="50" Click="InvBackBtn_Click"/>
        </StackPanel>
    </Grid>
</Page>
