<Page x:Class="Snap_Admin_System_Interface.AdminWindowParts.WPF_UI.SalesPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Snap_Admin_System_Interface.AdminWindowParts.WPF_UI"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="SalesPage">

    <Grid>
        <StackPanel Grid.Column="1" Grid.ColumnSpan="4" Grid.Row="1" >
            <Button x:Name="SalesBtn" Content="Sales" Height="50"/>
            <Button x:Name="CouponsBtn" Content="Coupons" Height="50"/>
            <Button x:Name="ListSalesBtn" Content="List of Sales" Height="50"/>
            <Button x:Name="ListCouponsBtn" Content="List of Coupons" Height="50"/>
            <Button x:Name="SalesBackBtn" Content="Back" Height="50"/>
        </StackPanel>
    </Grid>
</Page>
