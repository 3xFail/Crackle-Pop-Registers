<Page x:Class="Snap_Admin_System_Interface.AdminWindowParts.WPF_UI.AnalysisPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Snap_Admin_System_Interface.AdminWindowParts.WPF_UI"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="AnalysisPage">

    <Grid>
        <StackPanel Grid.Column="1" Grid.ColumnSpan="4" Grid.Row="1" Orientation="Horizontal" HorizontalAlignment="Center">
            <Button x:Name="GrossBtn" Content="Gross Earnings" Background="{StaticResource ButtonColor}" Height="50" Width="125" Margin="0,0,20,0"/>
            <Button x:Name="ProductBtn" Content="Product Losses" Background="{StaticResource ButtonColor}" Height="50" Width="125"/>
        </StackPanel>
    </Grid>
</Page>

