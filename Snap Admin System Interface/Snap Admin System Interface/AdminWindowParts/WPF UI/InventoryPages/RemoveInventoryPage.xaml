<Page x:Class="Snap_Admin_System_Interface.AdminWindowParts.WPF_UI.InventoryPages.RemoveInventoryPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Snap_Admin_System_Interface.AdminWindowParts.WPF_UI.InventoryPages"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="RemoveInventoryPage">

    <Grid Background="White" Margin="0,0,0,0">
        <TextBox x:Name="ItemIDBox" HorizontalAlignment="Left" Height="23" Margin="154,50,-54,0" TextWrapping="NoWrap" Text="" VerticalAlignment="Top" Width="200"/>
        <Label x:Name="IDLabel" Content="Item ID:" HorizontalAlignment="Left" Margin="67,51,0,0" VerticalAlignment="Top" Width="55"/>
        <Label x:Name="ItemInformationLabel" Content="Item Information:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" FontSize="17"/>
        <Button x:Name="SubmitButton" Content="Submit" HorizontalAlignment="Right" Margin="0,0,10,10" VerticalAlignment="Bottom" Width="75" Click="SubmitButton_Click"/>
        <Button x:Name="ResetButton" Content="Reset" HorizontalAlignment="Right" Margin="0,0,90,10" VerticalAlignment="Bottom" Width="75" Click="ResetButton_Click" />
    </Grid>
</Page>
