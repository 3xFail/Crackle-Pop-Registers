<Page x:Class="Snap_Admin_System_Interface.AdminWindowParts.WPF_UI.InventoryPages.CatalogInventoryPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Snap_Admin_System_Interface.AdminWindowParts.WPF_UI.InventoryPages"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="CatalogInventoryPage">

    <Grid>
        <DataGrid SelectionChanged="DataGrid_SelectionChanged">
            x:Name="Catalog" HorizontalAlignment="Left" Height="300" VerticalAlignment="Top" Width="300" 
            HeaderVisibility = "Column" CanUserSortColumns="True"
            AlternatingRowBackground="Gainsboro"  AlternationCount="2"
            SelectionMode="Extended" SelectionUnit="Row"
        </DataGrid>
                  
        

    </Grid>
</Page>
