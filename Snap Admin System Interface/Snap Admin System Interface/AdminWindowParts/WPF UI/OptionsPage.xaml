<Page x:Class="Snap_Admin_System_Interface.AdminWindowParts.WPF_UI.OptionsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Snap_Admin_System_Interface.AdminWindowParts.WPF_UI"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="OptionsPage">

    <Grid>
        <StackPanel Grid.Column="1" Grid.ColumnSpan="4" Grid.Row="1" >
            <Button x:Name="MngBtn" Content="Manage Registers" Height="50"/>
            <Button x:Name="LogoBtn" Content="Change Logo" Height="50"/>
            <Button x:Name="ColorBtn" Content="Change Color Scheme" Height="50"/>
        </StackPanel>
    </Grid>
</Page>
