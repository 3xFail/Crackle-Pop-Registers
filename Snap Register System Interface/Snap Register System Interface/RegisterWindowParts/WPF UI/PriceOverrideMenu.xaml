<Window x:Class="Snap_Register_System_Interface.PriceOverrideMenu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Snap_Register_System_Interface"
        mc:Ignorable="d"
        Title="PriceOverrideMenu" Height="400" Width="600"
		WindowStyle="None"
		ResizeMode="NoResize">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height=".5*"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>
        <Viewbox Grid.Row="0" HorizontalAlignment="Left">
			<TextBlock x:Name="ItemName" Text="Item Name"
					   Margin="10,10,10,10"
					   HorizontalAlignment="Left"
					   VerticalAlignment="Center"/>
		</Viewbox>
    </Grid>
</Window>
